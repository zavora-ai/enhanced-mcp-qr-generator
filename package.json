{
  "name": "enhanced-mcp-qr-generator",
  "version": "1.0.0",
  "description": "MCP server for generating QR codes from URLs and text",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "prepare": "husky install || echo 'Skipping husky install'",
    "prepublishOnly": "npm test && npm run lint",
    "docs": "typedoc --out docs/api src",
    "version:patch": "node scripts/version.js patch",
    "version:minor": "node scripts/version.js minor",
    "version:major": "node scripts/version.js major"
  },
  "keywords": [
    "mcp",
    "qr",
    "qrcode",
    "generator",
    "image",
    "url"
  ],
  "author": "James Karanja <james.karanja@zavora.ai> (https://www.zavora.ai)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zavora-ai/enhanced-mcp-qr-generator.git"
  },
  "bugs": {
    "url": "https://github.com/zavora-ai/enhanced-mcp-qr-generator/issues"
  },
  "homepage": "https://www.zavora.ai/enhanced-mcp-qr-generator",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "qrcode": "^1.5.3",
    "sharp": "^0.33.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^18.16.19",
    "@types/qrcode": "^1.5.5",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "bin": {
    "enhanced-mcp-qr-generator": "./dist/index.js"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
